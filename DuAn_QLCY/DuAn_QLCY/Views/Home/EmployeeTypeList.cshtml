@model IEnumerable<DuAn_QLCY.Models.EmployeeType>

@{
    ViewData["Title"] = "EmployeeTypeList";
}

<h1>EmployeeTypeList</h1>

<p>
    <a asp-action="CreateEmployeeType">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeName)
            </td>
            <td>
                    <a asp-action="UpdateEmployeeType" asp-route-id="@item.TypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TypeId">Details</a> |
                    <a asp-action="DeleteEmployeeType" asp-route-id="@item.TypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
