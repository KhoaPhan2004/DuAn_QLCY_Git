@model DuAn_QLCY.Models.EmployeeTechnology

@{
    ViewData["Title"] = "CreateEmployeeTechnologies";
}

<h1>CreateEmployeeTechnologies</h1>

<h4>EmployeeTechnology</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateEmployeeTechnologies">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EmployeeId" class="control-label"></label>
                <select asp-for="EmployeeId" class="form-control" asp-items=" ViewBag.EmployeesSelectList"></select>
            </div>
            <div class="form-group">
                <label asp-for="TechId" class="control-label"></label>
                <select asp-for="TechId" class="form-control" asp-items="ViewBag.TechnologiesSelectList"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProficiencyLevel" class="control-label"></label>
                <input asp-for="ProficiencyLevel" class="form-control" />
                <span asp-validation-for="ProficiencyLevel" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
